<?xml version="1.0" encoding="UTF-8"?>

<schemalist>
    <schema path="/org/gnome/shell/extensions/dock-express/" id="org.gnome.shell.extensions.dock-express">

    <key type="b" name="dock-autohide">
        <default>true</default>
        <summary>Autohide the dock</summary>
    </key>

    <key type="i" name="pressure-treshold">
        <default>100</default>
        <summary>Pressure treshold</summary>
    </key>

    <key type="i" name="animation-duration">
        <default>150</default>
        <summary>Animation duration</summary>
    </key>

    <key type="i" name="dimmed-opacity">
        <default>50</default>
        <summary>Dock dimmed opacity</summary>
    </key>

    </schema>
</schemalist>
